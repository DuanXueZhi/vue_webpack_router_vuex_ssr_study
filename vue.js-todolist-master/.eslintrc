{
  "extends": ["standard", "plugin:vue/recommended"],
  "plugins": [
    "html"
  ],
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "rules": {
    "no-new": "off",
    "space-before-function-paren": [2, "never"],
    "vue/max-attributes-per-line": ["error", {
      "singleline": 1,
      "multiline": {
        "max": 1,
        "allowFirstLine": false
      }
    }]
  }
}
